{
    "version": 2,
    "alias": "lemonaid-client-lemonaid-client",
    "name": "lemonaid-client",
    "routes": [
      {
        "src": "^/static/(.*)",
        "dest": "/static/$1"
      },
      { "src": "^/favicon.ico$",
        "dest": "/favicon.ico"
      },
      {
        "src": "^/manifest.json$",
        "dest": "/manifest.json"
       },
      {
        "src": ".*",
        "dest": "/index.html"
      }
    ]
  }